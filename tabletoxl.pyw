#HTML 테이블 소스코드를 엑셀로 만들어줍니다. 웹사이트상 등록된 내용을 검수할때 사용합니다.

import pandas as pd
from bs4 import BeautifulSoup

def html_table_to_excel(html_content, output_file):
    soup = BeautifulSoup(html_content, 'html.parser')
    tables = soup.find_all('table')

    dfs = []
    for table in tables:
        rows = table.find_all('tr')
        data = []
        for row in rows:
            cols = row.find_all(['td', 'th'])
            row_data = []
            for col in cols:
                # td 요소 내부의 텍스트를 가져와 콤마로 구분하여 추가
                divs = col.find_all('div')
                if divs:
                    cell_data = ','.join([div.get_text(strip=True) for div in divs])
                else:
                    cell_data = col.get_text(strip=True)
                row_data.append(cell_data)
            data.append(row_data)
        df = pd.DataFrame(data)
        dfs.append(df)

    final_df = pd.concat(dfs, ignore_index=True)

    final_df.to_excel(output_file, index=False)

